<a class="btn btn-default" href="<%= user_path(@user)%>/?from=<%= @previous_month.strftime("%d/%m/%Y") %>">
  <span class="glyphicon glyphicon-backward"></span>
  <%= @previous_month.strftime("%B %Y") %>
</a>

<a class="btn btn-default pull-right" href="<%= user_path(@user) %>/?from=<%= @next_month.strftime("%d/%m/%Y") %>">
  <%= @next_month.strftime("%B %Y") %>
  <span class="glyphicon glyphicon-forward"></span>
</a>
